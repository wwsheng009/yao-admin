{
  "action": {
    "bind": {
      "option": {
        "withs": {}
      },
      "model": "sys.domain"
    }
  },
  "layout": {
    "primary": "id",
    "actions": [
      {
        "showWhenView": true,
        "action": [
          {
            "type": "Studio.model",
            "payload": {
              "method": "CreateOne",
              "args": [
                "sys.domain"
              ]
            },
            "name": "StudioModel"
          },
          {
            "name": "Confirm",
            "type": "Common.confirm",
            "payload": {
              "title": "提示",
              "content": "处理完成"
            }
          }
        ],
        "title": "重新生成代码",
        "icon": "icon-layers",
        "showWhenAdd": true
      },
      {
        "action": [
          {
            "type": "Common.closeModal",
            "payload": {},
            "name": "CloseModal"
          }
        ],
        "title": "返回",
        "icon": "icon-arrow-left",
        "showWhenAdd": true,
        "showWhenView": true
      },
      {
        "action": [
          {
            "name": "Submit",
            "type": "Form.submit",
            "payload": {}
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ],
        "title": "保存",
        "icon": "icon-check",
        "style": "primary",
        "showWhenAdd": true
      },
      {
        "icon": "icon-trash-2",
        "style": "danger",
        "title": "Delete",
        "action": [
          {
            "payload": {
              "title": "提示",
              "content": "确认删除，删除后数据无法恢复？"
            },
            "name": "Confirm",
            "type": "Common.confirm"
          },
          {
            "name": "Delete",
            "payload": {
              "model": "sys.domain"
            },
            "type": "Form.delete"
          },
          {
            "payload": {},
            "name": "Back",
            "type": "Common.closeModal"
          }
        ]
      }
    ],
    "form": {
      "sections": [
        {
          "columns": [
            {
              "name": "name",
              "width": 8
            },
            {
              "name": "字段标题",
              "width": 8
            },
            {
              "name": "字段介绍",
              "width": 8
            },
            {
              "name": "字段注释",
              "width": 8
            },
            {
              "name": "字段长度",
              "width": 8
            },
            {
              "name": "字段位数(含小数位)",
              "width": 8
            },
            {
              "width": 8,
              "name": "字段小数位位数"
            },
            {
              "name": "字段是否可以为空",
              "width": 8
            },
            {
              "width": 8,
              "name": "string|number|float|字段默认值"
            },
            {
              "name": "字段加密存储方式",
              "width": 8
            }
          ]
        },
        {
          "columns": [
            {
              "width": 24,
              "name": "表格系统要素"
            }
          ]
        },
        {
          "columns": [
            {
              "name": "表格系统桌子领域",
              "width": 24
            }
          ]
        }
      ],
      "props": {}
    }
  },
  "fields": {
    "form": {
      "string|number|float|字段默认值": {
        "bind": "default",
        "edit": {
          "type": "Input",
          "props": {}
        }
      },
      "字段加密存储方式": {
        "edit": {
          "type": "Input",
          "props": {}
        },
        "bind": "crypt"
      },
      "表格系统桌子领域": {
        "bind": "id",
        "edit": {
          "type": "Table",
          "props": {
            "model": "sys.table.field",
            "query": {
              "where.domain_id.eq": "{{id}}"
            }
          }
        }
      },
      "字段小数位位数": {
        "bind": "scale",
        "edit": {
          "props": {},
          "type": "Input"
        }
      },
      "表格系统要素": {
        "bind": "id",
        "edit": {
          "props": {
            "model": "sys.element",
            "query": {
              "where.domain_id.eq": "{{id}}"
            }
          },
          "type": "Table"
        }
      },
      "字段标题": {
        "edit": {
          "type": "Input",
          "props": {}
        },
        "bind": "title"
      },
      "name": {
        "edit": {
          "type": "Input",
          "props": {}
        },
        "bind": "name"
      },
      "字段介绍": {
        "bind": "description",
        "edit": {
          "props": {},
          "type": "Input"
        }
      },
      "字段长度": {
        "bind": "length",
        "edit": {
          "props": {},
          "type": "Input"
        }
      },
      "字段是否可以为空": {
        "edit": {
          "props": {},
          "type": "Input"
        },
        "bind": "nullable"
      },
      "字段注释": {
        "bind": "comment",
        "edit": {
          "type": "TextArea",
          "props": {}
        }
      },
      "字段位数(含小数位)": {
        "bind": "precision",
        "edit": {
          "type": "Input",
          "props": {}
        }
      }
    }
  },
  "name": "领域"
}