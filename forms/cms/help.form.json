{
  "fields": {
    "form": {
      "category_id": {
        "bind": "category_id",
        "edit": {
          "type": "Input",
          "props": {}
        }
      },
      "icon": {
        "view": {
          "compute": "scripts.file.image.ImagesView",
          "props": {},
          "type": "Image"
        },
        "edit": {
          "type": "Upload",
          "compute": {
            "process": "scripts.file.image.ImagesEdit",
            "args": [
              "$C(row)",
              "$C(type)",
              "icon",
              "cms_help"
            ]
          },
          "props": {
            "$api": {
              "process": "fs.system.Upload"
            },
            "filetype": "image"
          }
        },
        "bind": "icon"
      },
      "title": {
        "edit": {
          "type": "Input",
          "props": {}
        },
        "bind": "title"
      },
      "show_status": {
        "bind": "show_status",
        "edit": {
          "props": {},
          "type": "Input"
        }
      },
      "create_time": {
        "edit": {
          "type": "DatePicker",
          "props": {}
        },
        "bind": "create_time"
      },
      "read_count": {
        "bind": "read_count",
        "edit": {
          "type": "Input",
          "props": {}
        }
      },
      "content": {
        "edit": {
          "props": {},
          "type": "TextArea"
        },
        "bind": "content"
      }
    }
  },
  "name": "帮助_类型",
  "action": {
    "bind": {
      "model": "cms.help",
      "option": {
        "withs": {}
      }
    }
  },
  "layout": {
    "primary": "id",
    "actions": [
      {
        "showWhenView": true,
        "action": [
          {
            "payload": {
              "args": [
                "cms.help"
              ],
              "method": "CreateOne"
            },
            "name": "StudioModel",
            "type": "Studio.model"
          },
          {
            "payload": {
              "title": "提示",
              "content": "处理完成"
            },
            "name": "Confirm",
            "type": "Common.confirm"
          }
        ],
        "title": "重新生成代码",
        "icon": "icon-layers",
        "showWhenAdd": true
      },
      {
        "title": "返回",
        "icon": "icon-arrow-left",
        "showWhenAdd": true,
        "showWhenView": true,
        "action": [
          {
            "name": "CloseModal",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "action": [
          {
            "name": "Submit",
            "type": "Form.submit",
            "payload": {}
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ],
        "title": "保存",
        "icon": "icon-check",
        "style": "primary",
        "showWhenAdd": true
      },
      {
        "icon": "icon-trash-2",
        "style": "danger",
        "title": "Delete",
        "action": [
          {
            "type": "Common.confirm",
            "payload": {
              "title": "提示",
              "content": "确认删除，删除后数据无法恢复？"
            },
            "name": "Confirm"
          },
          {
            "name": "Delete",
            "payload": {
              "model": "cms.help"
            },
            "type": "Form.delete"
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      }
    ],
    "form": {
      "sections": [
        {
          "columns": [
            {
              "width": 8,
              "name": "category_id"
            },
            {
              "name": "icon",
              "width": 24
            },
            {
              "name": "title",
              "width": 8
            },
            {
              "name": "show_status",
              "width": 8
            },
            {
              "name": "create_time",
              "width": 8
            },
            {
              "name": "read_count",
              "width": 8
            },
            {
              "name": "content",
              "width": 8
            }
          ]
        }
      ],
      "props": {}
    }
  }
}