  "relations": {
    "columns": {
      "type": "hasMany",
      "model": "ddic.model.column",
      "key": "model_id",
      "foreign": "id"
    },
    "relations": {
      "type": "hasMany",
      "model": "ddic.model.relation",
      "key": "model_id",
      "foreign": "id"
    }
  },
  "option": {}
