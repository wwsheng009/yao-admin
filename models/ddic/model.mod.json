{
  "name": "::Model",
  "table": { "name": "ddic_model", "comment": "业务模型" },
  "columns": [
    {
      "name": "id",
      "type": "ID",
      "label": "ID",
      "comment": "表名"
    },
    {
      "label": "命名空间",
      "name": "namespace",
      "type": "string",
      "length": 80,
      "default": "app",
      "nullable": false
    },
    {
      "label": "模型名称",
      "name": "name",
      "type": "string",
      "length": 80,
      "unique": true
    },
    {
      "label": "模型备注",
      "name": "model_comment",
      "type": "string",
      "length": 100,
      "comment": "模型定义注释",
      "nullable": true
    },
    {
      "label": "数据库表名",
      "name": "table_name",
      "type": "string",
      "length": 80,
      "unique": true
    },
    {
      "label": "数据库表备注",
      "name": "table_comment",
      "type": "string",
      "length": 100,
      "comment": "对应数据表中字段注释",
      "nullable": true
    },

    {
      "label": "软删除",
      "name": "soft_deletes",
      "type": "boolean",
      "default": true,
      "comment": "软删除，不直接删除"
    },
    {
      "label": "增加时间戳",
      "name": "timestamps",
      "type": "boolean",
      "default": true,
      "comment": "增加创建，更新时间戳"
    }
