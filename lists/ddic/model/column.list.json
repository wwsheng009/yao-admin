{
  "action": {
    "bind": {
      "table": "ddic.model.column"
    }
  },
  "layout": {
    "list": {
      "columns": [
        {
          "name": "名称",
          "width": 6
        },
        {
          "name": "数据元素",
          "width": 6
        },
        {
          "name": "标签",
          "width": 6
        },
        {
          "width": 6,
          "name": "字段类型"
        },
        {
          "name": "长度",
          "width": 6
        },
        {
          "name": "位数(含小数位)",
          "width": 6
        },
        {
          "width": 6,
          "name": "小数位"
        },
        {
          "name": "允许空值",
          "width": 6
        },
        {
          "name": "加密方式",
          "width": 6
        },
        {
          "width": 6,
          "name": "默认值"
        },
        {
          "name": "是否索引",
          "width": 6
        },
        {
          "name": "唯一索引",
          "width": 6
        },
        {
          "name": "注释",
          "width": 6
        }
      ]
    }
  },
  "fields": {
    "list": {
      "位数(含小数位)": {
        "bind": "precision",
        "edit": {
          "type": "InputNumber",
          "props": {
            "itemProps": {
              "tooltip": "字段位数(含小数位)precision，对 `float`、`decimal` 等类型字段有效"
            }
          }
        }
      },
      "加密方式": {
        "bind": "crypt",
        "edit": {
          "type": "Select",
          "props": {
            "options": [
              {
                "value": "PASSWORD",
                "label": "::PASSWORD"
              },
              {
                "label": "::AES",
                "value": "AES"
              }
            ],
            "placeholder": "请选择加密方式",
            "itemProps": {
              "tooltip": "字段加密存储方式。许可值 `AES(MySQL Only)`, `PASSWORD`"
            }
          }
        }
      },
      "标签": {
        "bind": "label",
        "edit": {
          "type": "Input",
          "props": {
            "itemProps": {
              "tooltip": "字段显示名称label，用于在管理表单，开发平台等成场景下呈现"
            }
          }
        }
      },
      "字段类型": {
        "edit": {
          "props": {
            "itemProps": {
              "tooltip": "字段类型定义type"
            }
          },
          "type": "Input"
        },
        "bind": "type"
      },
      "唯一索引": {
        "bind": "unique",
        "edit": {
          "type": "RadioGroup",
          "props": {
            "value": 0,
            "options": [
              {
                "label": "是",
                "value": 1
              },
              {
                "value": 0,
                "label": "否"
              }
            ],
            "itemProps": {
              "tooltip": "字段是否为唯一索引unique，默认为 false , 如为 true 无需同时将 `index` 设置为 true"
            },
            "defaultValue": 0
          }
        }
      },
      "名称": {
        "edit": {
          "props": {
            "itemProps": {
              "tooltip": "数据库表的字段名name"
            }
          },
          "type": "Input"
        },
        "bind": "name"
      },
      "注释": {
        "bind": "comment",
        "edit": {
          "type": "Input",
          "props": {
            "itemProps": {
              "tooltip": "注释，comment"
            }
          }
        }
      },
      "默认值": {
        "edit": {
          "props": {
            "itemProps": {
              "tooltip": "string|number|float|字段默认值default"
            }
          },
          "type": "Input"
        },
        "bind": "default"
      },
      "是否索引": {
        "bind": "index",
        "edit": {
          "type": "RadioGroup",
          "props": {
            "itemProps": {
              "tooltip": "字段是否为索引index，默认为 false"
            },
            "defaultValue": 0,
            "value": 0,
            "options": [
              {
                "label": "是",
                "value": 1
              },
              {
                "label": "否",
                "value": 0
              }
            ]
          }
        }
      },
      "长度": {
        "bind": "length",
        "edit": {
          "type": "InputNumber",
          "props": {
            "itemProps": {
              "tooltip": "字段长度length，对 `string` 等类型字段有效"
            }
          }
        }
      },
      "允许空值": {
        "edit": {
          "props": {
            "defaultValue": 0,
            "value": 0,
            "options": [
              {
                "label": "是",
                "value": 1
              },
              {
                "label": "否",
                "value": 0
              }
            ],
            "itemProps": {
              "tooltip": "是否允许空值nullable，默认为 false"
            }
          },
          "type": "RadioGroup"
        },
        "bind": "nullable"
      },
      "数据元素": {
        "edit": {
          "props": {
            "itemProps": {
              "tooltip": "数据元素ID，element_id"
            },
            "xProps": {
              "$remote": {
                "query": {
                  "value": "id",
                  "model": "ddic.element",
                  "label": "name"
                },
                "process": "yao.component.SelectOptions"
              }
            }
          },
          "type": "Select"
        },
        "bind": "element_id"
      },
      "小数位": {
        "edit": {
          "type": "InputNumber",
          "props": {
            "itemProps": {
              "tooltip": "字段小数位位数scale，对 `float`、`decimal` 等类型字段有效"
            }
          }
        },
        "bind": "scale"
      }
    }
  },
  "name": "::Model Column"
}